{
    "manifest_version": 2,
    "name": "Newpsel",
	"version": "1.0.8",

	"description": "Save page to later.",
	"icons": {
		"128": "icon.png"
	},

	"browser_action": {
		"default_icon": "img/defaultIcon19x19.png",
		"default_popup": "popup.html",
		"default_title": "Newpsel"
	},
	"content_scripts": [ {
        "js": [ "js/lib/jquery-1.9.1.min.js", "js/lib/jquery.cookie.js", "js/app/content.js" ],
        "matches": [ "*://*/*" ],
        "run_at": "document_start"
	} ],
	"minimum_chrome_version": "18",
	"permissions": [ "http://*/*", "https://*/*", "unlimitedStorage", "contextMenus", "cookies", "tabs", "notifications" ]

}